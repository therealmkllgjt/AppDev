{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app/dashboard/page.jsx",  //Corrected path - assuming page.jsx is at this location
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Next.js: debug",
            "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
            "args": ["dev"],
            "runtimeArgs": ["--inspect"],
            "autoAttachChildProcesses": true,
            "port": 9229
        }
    ]
}